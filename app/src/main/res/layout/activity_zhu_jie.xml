<?xml version="1.0" encoding="utf-8"?>

<layout>

    <data>

        <variable
            name="user"
            type="viewmodel.wondersgroup.com.mode.UserBind" />
        <variable
            name="padding"
            type="Integer" />
    </data>


    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".activity.ZhuJieActivity">

        <ImageView
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:imageUrl="@{user.headUrl}"
            android:placeHolder="@{@drawable/health_jump}"
            android:error="@{@drawable/received}"
            />

        <ImageView
            android:id="@+id/imageview"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:src="@{user.headUrl}"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hahahahahha"
            android:padd="@{padding}"/>


    </LinearLayout>
</layout>